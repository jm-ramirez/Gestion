  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT id.`codarticulo` codmaterial,m.nombre NombreMaterial,ic.fecha,p.nombre NombreProveedor,id.cantidad,id.precio
FROM ingresoscab ic LEFT JOIN ingresosdet id ON ic.id=id.idingreso
LEFT JOIN articulos m ON id.`codarticulo`=m.codigo LEFT JOIN proveedor p ON ic.`ProveedorId`=p.`id`
WHERE ic.tipo&lt;&gt;'E' AND ic.`Fecha`&gt;=@fdesde AND ic.`Fecha`&lt;=@fhasta
AND CASE @filtra
	WHEN '0' THEN id.`codarticulo` LIKE '%%'
	WHEN '1' THEN id.`codarticulo`=@codMat
END ORDER BY id.`codarticulo`,ic.fecha</CommandText>
        <QueryParameters>
          <QueryParameter Name="@fdesde">
            <Value>=Parameters!fdesde</Value>
          </QueryParameter>
          <QueryParameter Name="@fhasta">
            <Value>=Parameters!fhasta</Value>
          </QueryParameter>
          <QueryParameter Name="@filtra">
            <Value>=Parameters!filtra</Value>
          </QueryParameter>
          <QueryParameter Name="@codMat">
            <Value>=Parameters!codMat</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
